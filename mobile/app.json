{
  "expo": {
    "name": "Eventide AI",
    "slug": "eventide-ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#FFF8F5"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.eventideai.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFF8F5"
      },
      "package": "com.eventideai.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "*"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Eventide AI to access your camera to capture event flyers."
        }
      ]
    ]
  }
}

